{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Demo App",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Developer Name",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Demo App"
  },
  "permissions": ["GameInfo"],
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
          "is_background_page": false,
					"file":"build/index.html",
					"transparent": true,
					"resizable": false,
          "use_os_windowing": false,
					"size": {
						"width":700,
						"height":2000
						},
					"min_size": {
						"width":700,
						"height":2000
					}
				}
			
		}
	}
}
